{
  "name": "bc-dl",
  "version": "0.1.0",
  "description": "youtube-dl for Bandcamp, with mp3 tags",
  "repository": "https://github.com/Gurimarukin/bc-dl.git",
  "author": "Gurimarukin",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "22.14.0"
  },
  "packageManager": "yarn@4.8.1",
  "scripts": {
    "dl": "vite-node src/dl.ts",
    "tag": "vite-node src/tag.ts",
    "~compile": "tsc -w --preserveWatchOutput",
    "_prettier": "prettier --cache .",
    "formatCheck": "yarn _prettier --check",
    "format": "yarn _prettier --write",
    "lint": "eslint . --cache --max-warnings 0",
    "lintFix": "yarn lint --fix",
    "jestUnit": "jest --testPathPattern=\".*\\.spec\\.ts\"",
    "jestCoverage": "jest --ci --coverage",
    "ci": "yarn tsc && yarn formatCheck && yarn lint && yarn jestUnit"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/jest": "29.5.14",
    "@types/jsdom": "16.2.6",
    "axios": "0.21.1",
    "decline-ts": "0.9.0",
    "eslint": "9.23.0",
    "eslint-plugin-fp-ts": "0.4.0",
    "eslint-plugin-functional": "9.0.1",
    "fp-ts": "2.11.7",
    "io-ts": "2.2.13",
    "jest": "29.7.0",
    "jsdom": "16.4.0",
    "monocle-ts": "2.3.5",
    "newtype-ts": "0.3.4",
    "node-id3": "0.2.2",
    "prettier": "3.5.3",
    "ts-jest": "29.3.1",
    "ts-node": "9.1.1",
    "tslib": "2.8.1",
    "typescript": "5.8.2",
    "typescript-eslint": "8.28.0"
  }
}
