{
  "name": "bc-dl",
  "version": "0.1.0",
  "description": "youtube-dl for Bandcamp, with mp3 tags",
  "main": "lib/index.js",
  "repository": "https://github.com/Gurimarukin/bc-dl.git",
  "author": "Gurimarukin",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "dl": "node --unhandled-rejections=strict lib/dl.js",
    "tag": "node --unhandled-rejections=strict lib/tag.js",
    "tsDl": "NODE_OPTIONS='--unhandled-rejections=strict' ts-node src/dl.ts",
    "tsTag": "NODE_OPTIONS='--unhandled-rejections=strict' ts-node src/tag.ts",
    "prettier": "prettier --check \"./{src,test}/**/*.ts\"",
    "prettierFix": "prettier --write \"./{src,test}/**/*.ts\"",
    "lint": "eslint --cache --max-warnings 0 --ext .ts src test",
    "lintFix": "eslint --fix --cache --max-warnings 0 --ext .ts src test",
    "jest": "jest",
    "jestUnit": "jest --testPathPattern=.*\\.spec\\.ts",
    "jestCoverage": "jest --ci --coverage",
    "style": "yarn run prettier && yarn run lint",
    "test": "yarn run style && yarn run jestUnit"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.1.1",
    "@type-config/strict": "1.1.1",
    "@types/jest": "26.0.20",
    "@types/jsdom": "16.2.6",
    "@types/rimraf": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "axios": "0.21.1",
    "decline-ts": "0.9.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-fp-ts": "0.3.0",
    "eslint-plugin-functional": "4.0.2",
    "eslint-plugin-prettier": "4.0.0",
    "fp-ts": "2.11.7",
    "io-ts": "2.2.13",
    "jest": "26.6.3",
    "jsdom": "16.4.0",
    "monocle-ts": "2.3.5",
    "newtype-ts": "0.3.4",
    "node-id3": "0.2.2",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
}
