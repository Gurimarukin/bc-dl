{
  "name": "bc-dl",
  "version": "0.1.0",
  "description": "youtube-dl for Bandcamp, with mp3 tags",
  "repository": "https://github.com/Gurimarukin/bc-dl.git",
  "author": "Gurimarukin",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "22.14.0"
  },
  "packageManager": "yarn@4.8.1",
  "scripts": {
    "dl": "vite-node src/dl.ts",
    "tag": "vite-node src/tag.ts",
    "~compile": "tsc -w --preserveWatchOutput",
    "_prettier": "prettier --cache .",
    "formatCheck": "yarn _prettier --check",
    "format": "yarn _prettier --write",
    "lint": "eslint . --cache --max-warnings 0",
    "lintFix": "yarn lint --fix",
    "jestUnit": "jest --testPathPattern=.*\\.spec\\.ts"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/jest": "26.0.20",
    "@types/jsdom": "16.2.6",
    "@types/rimraf": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "axios": "0.21.1",
    "decline-ts": "0.9.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-fp-ts": "0.3.0",
    "eslint-plugin-functional": "4.0.2",
    "fp-ts": "2.11.7",
    "io-ts": "2.2.13",
    "jest": "26.6.3",
    "jsdom": "16.4.0",
    "monocle-ts": "2.3.5",
    "newtype-ts": "0.3.4",
    "node-id3": "0.2.2",
    "prettier": "3.5.3",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "tslib": "2.8.1",
    "typescript": "5.8.2"
  }
}
